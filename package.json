{
  "name": "@trustpool/mobile-app",
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "expo": "expo",
    "start": "expo start",
    "start:android": "run-s \"start --android\"",
    "start:ios": "run-s \"start --ios\"",
    "publish": "expo publish -c",
    "build": "run-s \"build:android {@}\" \"build:ios {@}\" --",
    "build:android": "expo build:android -t apk --no-publish --no-wait",
    "build:ios": "expo build:ios -t archive --no-publish --no-wait",
    "deploy": "run-s \"publish {@}\" \"build:android {@}\" \"build:ios {@}\" --",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "lint:fix": "run-s \"lint --fix\"",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19000' && expo start -i --clear",
    "prepare": "husky install"
  },
